#%Module

proc ModulesHelp { } {
    puts stderr {   The grep command searches one or more input files for lines containing a match to a specified pattern. By default, grep prints the matching lines. - Homepage: http://www.gnu.org/software/grep/
    }
}

module-whatis {Description: The grep command searches one or more input files for lines containing a match to a specified pattern. By default, grep prints the matching lines. - Homepage: http://www.gnu.org/software/grep/}

set root    /apps/leuven/thinking/2015a/software/Grep/2.21-GCC-4.9.2

conflict Grep

if { ![is-loaded GCC/4.9.2] } {
    module load GCC/4.9.2
}

prepend-path	MANPATH		$root/share/man
prepend-path	PATH		$root/bin

setenv	EBROOTGREP		"$root"
setenv	EBVERSIONGREP		"2.21"
setenv	EBDEVELGREP		"$root/easybuild/Grep-2.21-GCC-4.9.2-easybuild-devel"


